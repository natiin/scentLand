<% layout('layouts/boilerplate') %>
<div class="container">
  <div class="row">
    <div class="col-6 offset-3">
      <div class="alert alert-danger" role="alert">
        <p>Error <%= err.message %></p>
        <% if(process.env.NODE_ENV !== "production") {%>
        <p><%= err.stack%></p>
        <% } %>
      </div>
    </div>
  </div>
</div>
